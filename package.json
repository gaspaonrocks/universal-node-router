{
  "name": "universal-node-router",
  "version": "0.4.0",
  "description": "a simple router that matches requests to corresponding backend controllers",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "cover": "nyc npm run test"
  },
  "keywords": [
    "node",
    "router",
    "express"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:gaspaonrocks/universal-node-router.git"
  },
  "engines": {
    "node": "8.x",
    "npm": "5.x"
  },
  "author": "gaspaonrocks",
  "license": "ISC",
  "dependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/express": "^4.11.0",
    "@types/node": "^9.3.0",
    "express": "^4.16.2",
    "fs": "0.0.1-security"
  },
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/mocha": "^2.2.46",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "supertest": "^3.0.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2"
  },
  "nyc": {
    "require": [
      "ts-node/register"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
